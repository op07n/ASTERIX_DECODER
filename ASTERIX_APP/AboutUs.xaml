        <Image Grid.Column="3" HorizontalAlignment="Right" Height="100" Margin="0,40,35,0" Source="./Images/judit.jpg" VerticalAlignment="Top" Width="100"/>
        <Image Grid.Column="4" HorizontalAlignment="Right" Height="100" Margin="0,40,35,0" Source="./Images/santi.jpg" VerticalAlignment="Top" Width="100"/>
        <Label Grid.Column="0" HorizontalAlignment="Right" Content="Ariadna Santaella Cáceres" VerticalAlignment="Top" Margin="0,170,15,0"  Background="#222" Foreground="#fff"/>
        <Label Grid.Column="0" HorizontalAlignment="Right" Content="Age : 22" VerticalAlignment="Top" Margin="0,210,60,0"  Background="#222" Foreground="#fff"/>
        <Label Grid.Column="1" HorizontalAlignment="Right" Content="Carola Moltó Suazo" VerticalAlignment="Top" Margin="0,170,25,0"  Background="#222" Foreground="#fff"/>
        <Label Grid.Column="1" HorizontalAlignment="Right" Content="Age : 21" VerticalAlignment="Top" Margin="0,210,60,0"  Background="#222" Foreground="#fff"/>
        <Label Grid.Column="2" HorizontalAlignment="Right" Content="Joan Hernández Font" VerticalAlignment="Top" Margin="0,170,25,0"  Background="#222" Foreground="#fff"/>
        <Label Grid.Column="2" HorizontalAlignment="Right" Content="Age : 21" VerticalAlignment="Top" Margin="0,210,60,0"  Background="#222" Foreground="#fff"/>
        <Label Grid.Column="3" HorizontalAlignment="Right" Content="Judith Pariente Mejias" VerticalAlignment="Top" Margin="0,170,25,0"  Background="#222" Foreground="#fff"/>
        <Label Grid.Column="3" HorizontalAlignment="Right" Content="Age : 21" VerticalAlignment="Top" Margin="0,210,60,0"  Background="#222" Foreground="#fff"/>
        <Label Grid.Column="4" HorizontalAlignment="Right" Content="Santiago del Hierro" VerticalAlignment="Top" Margin="0,170,30,0"  Background="#222" Foreground="#fff"/>
